prototype(Liiioooo.DAProject:Content.TextHero) < prototype(Neos.Neos:ContentComponent) {
    @context.headline = Neos.Neos:Editable {
        property = 'headline'
    }

    renderer = afx`
        <div class="text-hero">
            {headline}
        </div>
    `

    @process.styles = Lio.PrototypeStyles:LoadStyles {
        file = 'resource://Liiioooo.DAProject/Public/css-single/Content/text-hero.css'
    }
}
